# AI Service API Keys
OPENAI_API_KEY=your_openai_api_key_here
STABILITY_API_KEY=your_stability_api_key_here

# Gemini 配置 (中转站 API)
GEMINI_API_KEY=
GEMINI_BASE_URL=https://yunwu.ai
# Gemini 模型选项 (可在前端选择):
# - gemini-2.5-flash-image (推荐，快速生成)
# - gemini-3-pro-image-preview (高级版本，支持 4K、Google Search)
GEMINI_MODEL=gemini-3-pro-image-preview 

# Doubao 配置
DOUBAO_API_KEY=
DOUBAO_BASE_URL=https://yunwu.ai
DOUBAO_MODEL=doubao-seedream-4-0-250828

# Xiaomi MiMO Chat 配置
MIMO_API_KEY=
MIMO_BASE_URL=https://api.xiaomimimo.com/v1
MIMO_MODEL=mimo-v2-flash
MIMO_TEMPERATURE=0.8
MIMO_TOP_P=0.95
MIMO_MAX_TOKENS=1024

# Sora 视频生成配置
SORA_API_KEY=
SORA_BASE_URL=https://yunwu.ai
SORA_MODEL=sora-2

# VEO 视频生成配置 (Google)
VEO_API_KEY=
VEO_BASE_URL=https://yunwu.ai
# VEO 模型选项: veo2, veo2-fast, veo3, veo3-fast, veo3-pro, veo3-fast-frames 等
VEO_MODEL=veo3-fast

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=True

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,http://127.0.0.1:5500,http://localhost:5500
CHAT_API_KEY=
CHAT_BASE_URL=https://yunwu.ai/v1
CHAT_MODEL=gpt-5-2025-08-07
